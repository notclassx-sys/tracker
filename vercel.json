{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.mjs"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/index.html"
        }
    ]
}